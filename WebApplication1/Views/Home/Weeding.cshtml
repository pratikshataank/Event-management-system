

@{
    ViewBag.Title = "Gallary Page";
}

<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Photo Gallery</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link href="https://fonts.googleapis.com/css?family=Droid+Sans:400,700" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.css">
    <link rel="stylesheet" href="~/Content/Gallery.css">


</head>
<body>

    <div class="container gallery-container">

        <h1 class="page-description text-center">Weeding</h1>
        <!--Weeding event-->

       
        <div class="tz-gallery">

            <div class="row">

                <div class=" col-md-4">
                    <a class="lightbox" href="~/Content/Images/Galleryimage/g1.jpg">
                        <img src="~/Content/Images/Galleryimage/g1.jpg" alt="Bridge">
                    </a>
                </div>
                <div class=" col-md-4">
                    <a class="lightbox" href="~/Content/Images/Galleryimage/g2.jpg">
                        <img src="~/Content/Images/Galleryimage/g2.jpg" alt="Park">
                    </a>
                </div>
                <div class=" col-md-4">
                    <a class="lightbox" href="~/Content/Images/Galleryimage/g3.jpg">
                        <img src="~/Content/Images/Galleryimage/g3.jpg" alt="Tunnel">
                    </a>
                </div>
                <div class="col-md-4">
                    <a class="lightbox" href="~/Content/Images/Galleryimage/g4.jpg">
                        <img src="~/Content/Images/Galleryimage/g4.jpg" alt="Traffic">
                    </a>
                </div>
                <div class=" col-md-4">
                    <a class="lightbox" href="~/Content/Images/Galleryimage/g5.jpg">
                        <img src="~/Content/Images/Galleryimage/g5.jpg" alt="Coast">
                    </a>
                </div>
                <div class="col-md-4">
                    <a class="lightbox" href="~/Content/Images/Galleryimage/g6.jpg">
                        <img src="~/Content/Images/Galleryimage/g6.jpg" alt="Rails">
                    </a>
                </div>

            </div>

        </div><hr />

        

    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.js"></script>
    <script>
        baguetteBox.run('.tz-gallery');
    </script>
</body>
</html>
